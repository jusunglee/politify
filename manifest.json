{
    "manifest_version": 2,
    "name": "Politify",
    "version": "1.0",
    "description": "My first Chrome extension.",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery-3.2.1.min.js", "nhpup_1.1.js", "main.js", "test.js"], 
          "css": ["style.css"]
        
        }],
        
        "permissions": ["<all_urls>"]
  }